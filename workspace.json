    "dfobobcat-ui": {
      "projectType": "application",
      "root": "apps/dfobobcat-ui",
      "sourceRoot": "apps/dfobobcat-ui/src",
      "prefix": "bc",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/dfobobcat-ui",
            "index": "apps/dfobobcat-ui/src/index.html",
            "main": "apps/dfobobcat-ui/src/main.ts",
            "polyfills": "apps/dfobobcat-ui/src/polyfills.ts",
            "tsConfig": "apps/dfobobcat-ui/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/dfobobcat-ui/src/assets",
              "apps/dfobobcat-ui/src/.htaccess",
              {
                "glob": "**/*.svg",
                "input": "node_modules/ionicons/dist/ionicons/svg",
                "output": "./svg"
              }
            ],
            "styles": [
              "apps/dfobobcat-ui/src/styles.scss",
              "node_modules/perfect-scrollbar/css/perfect-scrollbar.css",
              {
                "input": "apps/dfobobcat-ui/src/theme/variables.scss"
              },
              "node_modules/@angular/cdk/overlay-prebuilt.css"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["apps/dfobobcat-ui/src/theme"]
            },
            "scripts": []
          },
          "configurations": {
            "staging": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputPath": "dist/apps/dfobobcat-ui/staging",
              "fileReplacements": [
                {
                  "replace": "apps/dfobobcat-ui/src/environments/environment.ts",
                  "with": "apps/dfobobcat-ui/src/environments/environment.staging.ts"
                }
              ]
            },
            "production": {
              "outputPath": "dist/apps/dfobobcat-ui/production",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/dfobobcat-ui/src/environments/environment.ts",
                  "with": "apps/dfobobcat-ui/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "options": {
            "disableHostCheck": true
          },
          "configurations": {
            "production": {
              "browserTarget": "dfobobcat-ui:build:production"
            },
            "development": {
              "browserTarget": "dfobobcat-ui:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "dfobobcat-ui:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/dfobobcat-ui/src/**/*.ts",
              "apps/dfobobcat-ui/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/dfobobcat-ui"],
          "options": {
            "jestConfig": "apps/dfobobcat-ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "cap": {
          "executor": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "--help"
          }
        },
        "add": {
          "executor": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "add",
            "packageInstall": true
          },
          "configurations": {
            "ios": {
              "cmd": "add ios"
            },
            "android": {
              "cmd": "add android"
            }
          }
        },
        "copy": {
          "executor": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "copy",
            "packageInstall": false
          },
          "configurations": {
            "ios": {
              "cmd": "copy ios"
            },
            "android": {
              "cmd": "copy android"
            }
          }
        },
        "open": {
          "executor": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "open",
            "packageInstall": false
          },
          "configurations": {
            "ios": {
              "cmd": "open ios"
            },
            "android": {
              "cmd": "open android"
            }
          }
        },
        "sync": {
          "executor": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "sync",
            "packageInstall": true
          },
          "configurations": {
            "ios": {
              "cmd": "sync ios"
            },
            "android": {
              "cmd": "sync android"
            }
          }
        },
        "update": {
          "executor": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "update",
            "packageInstall": true
          },
          "configurations": {
            "ios": {
