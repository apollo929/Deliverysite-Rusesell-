          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "dfobobcat-api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/dfobobcat-api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
