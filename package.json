    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "typegenold": "node -r ts-node/register ./tools/generate-types.ts",
    "typegen": "graphql-codegen --config=codegen.yml -e",
    "seed": "node tools/seeding/main.js",
    "clean": "sh -c \"psql --username=admin -h localhost -p 6432   -d dfobobcat -f ./clean.sql\"",
    "schema:drop": "ts-node -P ./libs/api/core/src/lib/database/tsconfig.json ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node -P ./libs/api/core/src/lib/database/tsconfig.json ./node_modules/typeorm/cli.js schema:sync",
    "postinstall": "ngcc --properties es2015 browser module main",
    "server-up": "angular-http-server --silent --path ./dist/apps/dfobobcat-ui/ -p 8080",
    "serve:fe": "nx serve dfobobcat-ui",
    "build:fe": "nx build dfobobcat-ui",
    "serve:be": "nx serve dfobobcat-api",
    "build:be:prod": "nx build dfobobcat-api --prod",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js  --config ormconfig.ts",
    "migrate": "node --require ts-node/register ./node_modules/typeorm/cli.js  --config ormconfig.ts migration:run",
    "mjml": "node generate-emails.js",
    "build:stg": "concurrently 'nx build dfobobcat-ui --configuration=staging' 'nx build dfobobcat-api --prod'",
    "build:prod": "concurrently 'nx build dfobobcat-ui --configuration=production' 'nx build dfobobcat-api --prod'",
    "build:all": "nx build dfobobcat-ui --configuration=staging; nx build dfobobcat-ui --configuration=production; nx build dfobobcat-api --prod",
    "build": "./build.sh"
  },
